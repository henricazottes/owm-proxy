{
    "name": "OWM Proxy",
    "description": "An open weather map proxy to reduce API results",
    "repository": "https://github.com/henricazottes/owm-proxy",
    "keywords": ["node", "express", "api"],
    "env": {
        "API_KEY": {
            "description": "Your Open Weather Map API Key.",
            "required": false
        },
        "CITY": {
            "description": "City name, in the Open Weather Map format.",
            "value": "Toulouse,fr",
            "required": false
        },
        "BASE_URL": {
            "descriptions": "Open Wether Map API base url.",
            "value": "https://api.openweathermap.org/data/2.5",
            "required": false
        },
        "UNITS": {
            "description": "Value units: metric or imperial.",
            "value": "metric",
            "required": false
        }
    }
  }
  